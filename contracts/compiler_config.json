{
    "language": "Solidity",
    "settings": {
        "optimizer": {
            "enabled": true,
            "runs": 200,
            "details": {
                "peephole": true,
                "inliner": true,
                "jumpdestRemover": true,
                "orderLiterals": true,
                "deduplicate": true,
                "cse": true,
                "constantOptimizer": true,
                "simpleCounterForLoopUncheckedIncrement": true,
                "yul": true,
                "yulDetails": {
                    "stackAllocation": true
                }
            }
        },
        "evmVersion": "cancun",
        "viaIR": true,
        "debug": {
            "revertStrings": "strip"
        },
        "metadata": {
            "appendCBOR": false,
            "useLiteralContent": true,
            "bytecodeHash": "none"
        },
        "outputSelection": {
            "*": {
                "*": ["evm.bytecode", "abi"]
            }
        }
    }
}
